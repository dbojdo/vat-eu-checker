{
    "name": "goosfraba/vies-eu-vat-checker",
    "description": "Verifies the validity of a VAT number issued by any Member State",
    "type": "library",
    "license": "MIT",
    "keywords": ["vat", "eu", "validator", "checker", "vies"],
    "require": {
        "php": ">=5.4",
        "webit/soap-api": "^2.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.36"
    },
    "autoload": {
        "psr-4": {
            "Goosfraba\\ViesEuVatChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\ViesEuVatChecker\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
